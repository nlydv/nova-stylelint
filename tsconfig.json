{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "strict": false,
        "allowJs": true,
        "checkJs": false,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "allowUnreachableCode": true,
        "forceConsistentCasingInFileNames": true
    }
}
